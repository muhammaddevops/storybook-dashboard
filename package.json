    "chroma-js": "^2.4.2",
    "d3": "^7.6.1",
    "faker": "^6.6.6",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^17.0.2",
    "react-octicon": "^3.0.1",
    "react-refresh": "0.11.0",
