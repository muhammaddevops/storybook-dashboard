    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.11.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
